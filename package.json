{
  "name": "minelaup-server",
  "version": "0.4.0",
  "description": "The server side of the project MineLaup",
  "author": "Louis Bailleau",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .ts,.js,.vue --ignore-path .gitignore . --fix",
    "test": "NODE_ENV=test jest",
    "test:unit": "cross-env TEST=unit jest ./test/specs/**",
    "test:e2e": "cross-env TEST=e2e jest ./test/e2e/**",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/axios": "^5.10.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "body-parser": "^1.19.0",
    "conf": "^6.2.4",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "nuxt": "^2.12.2",
    "nuxt-i18n": "^6.10.1",
    "nuxt-property-decorator": "^2.7.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.7",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.1.1",
    "tailwindcss-dark-mode": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@nuxtjs/tailwindcss": "^1.4.1",
    "@types/bluebird": "^3.5.30",
    "@types/jest": "^25.2.1",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "browser-env": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.3",
    "nuxt-types": "^0.2.11",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.4.0",
    "vue-jest": "^3.0.5"
  }
}
